{
  "name": "@northwood-labs/devsec-tools-sdk",
  "version": "0.1.0",
  "description": "A TypeScript SDK for interacting with the DevSecTools API.",
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.cjs",
    "types": "./dist/index.d.ts"
  },
  "scripts": {
    "build": "tsc && mv dist/index.js dist/index.mjs && cp dist/index.mjs dist/index.cjs",
    "clean": "rm -rf dist",
    "lint": "eslint . --ext .ts",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/northwood-labs/devsec-tools-sdk-ts.git"
  },
  "keywords": [
    "typescript",
    "sdk",
    "devsec",
    "security",
    "api-client"
  ],
  "author": "Northwood Labs",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.26.0",
    "@typescript-eslint/parser": "^8.26.0",
    "typescript": "^5.8.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.6",
    "eslint": "^9.21.0",
    "rimraf": "^6.0.1"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
